<odoo>
    <!-- Sequence for Local Purchase Request -->
    <record id="seq_local_purchase_request" model="ir.sequence">
        <field name="name">Local Payment Request</field>
        <field name="code">local.payment.request</field>
        <field name="prefix">PMR-%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>